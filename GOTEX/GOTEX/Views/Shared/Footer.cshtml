            $(".header").removeClass("changeBG");

        });
        $("#no").on('click', function (e) {
            $(".confirm").hide();
            $("#fade").removeClass("changeBG");
            $("#SB").removeClass("changeBG");
            $(".header").removeClass("changeBG");

        })
        $(".mcloseconfirm").on('click', function (e) {
            $("#mconfirm").hide();
        })
            $("#mno").on('click', function (e) {
            $("#mconfirm").hide();
        })
    })

</script>
